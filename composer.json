{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bavix/laravel-clickhouse": "^2.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "realrashid/sweet-alert": "^5.0",
        "spatie/enum": "^3.12",
        "spatie/laravel-permission": "^3.3",
        "yajra/laravel-datatables-buttons": "4.10",
        "yajra/laravel-datatables-oracle": "~9.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "joshbrw/laravel-module-installer": "^1.0",
        "kalnoy/nestedset": "^6.0",
        "league/csv": "^9.6",
        "nwidart/laravel-modules": "^7.1",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/laravel-enum": "^2.5",
        "whf/base-module": "dev-develop",
        "whf/entity-module": "dev-develop",
        "whf/media-module": "dev-develop",
        "whf/notifier-module": "dev-develop",
        "whf/user-entity-module": "dev-develop",
        "whf/user-module": "dev-develop"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/theme.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://karigar.repo.repman.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
